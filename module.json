{
    "name": "mana-spells",
    "title": "Patria Mori Mana Spells add-on module",
    "description": "This module extends the mana-circles module by adding support for spells",
    "authors": [
        {
            "name": "Jonas Drageset Lindaas"
        }
    ],
    "version": "0.1.0",
    "compatibility": {
        "minimum": 10,
        "verified": 10,
        "maximum": 11
    },
    "scripts": [
        "scripts/moduleConfig.js",
        "scripts/moduleHooks.js",
        "scripts/moduleSettings.js"
    ],
    "languages": [
        {
            "lang": "en",
            "name": "English",
            "path": "languages/en.json"
        }
    ],
    "styles": [
        "styles/mana-spells.css"
    ],
    "url": "https://github.com/Patria-Mori/mana-spells",
    "relationships": {
        "systems": [
            {
                "id": "dnd5e",
                "type": "system",
                "manifest": "https://raw.githubusercontent.com/foundryvtt/dnd5e/master/system.json",
                "compatibility": {
                    "verified": "2.1.5"
                }
            }
        ],

        "requires": [
            {
                "id": "mana-circles",
                "type": "module",
                "manifest": "https://raw.githubusercontent.com/Patria-Mori/mana-circles/main/module.json",
                "compatibility": {
                    "verified": "0.1.0"
                }
            }
        ]
    }
}